<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>H</servlet-name>
		<servlet-class>excelsior.battlehack.h.HServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserSignUp</servlet-name>
		<servlet-class>excelsior.battlehack.h.UserSignUpServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegisterParkingLot</servlet-name>
		<servlet-class>excelsior.battlehack.h.RegisterParkingLotServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegisterParkingSpot</servlet-name>
		<servlet-class>excelsior.battlehack.h.RegisterParkingSpotServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>H</servlet-name>
		<url-pattern>/h</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserSignUp</servlet-name>
		<url-pattern>/userSignUp</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterParkingLot</servlet-name>
		<url-pattern>/registerParkingLot</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterParkingSpot</servlet-name>
		<url-pattern>/registerParkingSpot</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
